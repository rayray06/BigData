<?xml version="1.0" encoding="UTF-8"?><assembly xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
    <id>bin</id>
    <includeBaseDirectory>false</includeBaseDirectory>
    <formats>
        <format>zip</format>
    </formats>
    <files>
        <file>
            <!-- jobInfo.properties -->
            <source>${basedir}/src/main/templates/jobInfo_template.properties
			</source>
            <outputDirectory>.</outputDirectory>
            <destName>jobInfo.properties</destName>
            <filtered>true</filtered>
        </file>
        <file>
            <!-- sh -->
            <source>${basedir}/src/main/templates/Job_run_template.sh</source>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <destName>${talend.job.name}_run.sh</destName>
            <filtered>true</filtered>
        </file>
        <file>
            <!-- bat -->
            <source>${basedir}/src/main/templates/Job_run_template.bat</source>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <destName>${talend.job.name}_run.bat</destName>
            <filtered>true</filtered>
        </file>
        <file>
            <!-- ps1 -->
            <source>${basedir}/src/main/templates/Job_run_template.ps1</source>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <destName>${talend.job.name}_run.ps1</destName>
            <filtered>true</filtered>
        </file>
    </files>
    <fileSets>
        <fileSet>
            <!-- templates -->
            <directory>${templates.dir}</directory>
            <outputDirectory>${talend.job.name}/src/main/templates/
			</outputDirectory>
            <includes>
                <include>**</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- log4j resources -->
            <directory>${log4j.resources.dir}</directory>
            <outputDirectory>${output.resources.dir}/
			</outputDirectory>
            <includes>
                <include>log4j.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- log4j.xml for running -->
            <directory>${log4j.running.dir}</directory>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <includes>
                <include>log4j.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- xmlMappings resources -->
            <directory>${xmlMappings.resources.dir}</directory>
            <outputDirectory>${output.resources.dir}/xmlMappings/
			</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- xmlMappings for running -->
            <directory>${xmlMappings.running.dir}</directory>
            <outputDirectory>${talend.job.name}/xmlMappings/
			</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- rules resources -->
            <directory>${rules.resources.dir}</directory>
            <outputDirectory>${output.resources.dir}/rules/
			</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- assembly_*.xml -->
            <directory>${assemblies.dir}</directory>
            <outputDirectory>${talend.job.name}/src/main/assemblies/
			</outputDirectory>
            <includes>
                <include>assembly.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- pom_*.xml -->
            <directory>${poms.dir}</directory>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <includes>
                <include>pom.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <!--parent pom.xml -->
            <directory>${parentpom.dir}</directory>
            <outputDirectory>.</outputDirectory>
            <includes>
                <include>pom.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <!--routines code -->
            <directory>${routinescode.dir}</directory>
            <outputDirectory>code/routines/src/main</outputDirectory>
            <includes>
                <include>**/**/*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <!--routines pom -->
            <directory>${routinespom.dir}</directory>
            <outputDirectory>code/routines/</outputDirectory>
            <includes>
                <include>pom.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <!--pigudf code -->
            <directory>${pigudfcode.dir}</directory>
            <outputDirectory>code/pigudf/src/main</outputDirectory>
            <includes>
                <include>**/**/*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <!--pifudf pom -->
            <directory>${pigudfpom.dir}</directory>
            <outputDirectory>code/pigudf/</outputDirectory>
            <includes>
                <include>pom.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <!--joblet -->
            <directory>${joblet.dir}</directory>
            <outputDirectory>jobs/</outputDirectory>
            <includes>
                <include>joblets*/**/*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- add items -->
            <directory>${items.dir}</directory>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <includes>
                <include>**</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- add java source codes -->
            <directory>${sourcecodes.dir}</directory>
            <outputDirectory>${talend.job.name}/src/main/java/</outputDirectory>
            <includes>
                <include>${talend.job.path}/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- add context resources -->
            <directory>${resources.dir}</directory>
            <outputDirectory>${output.resources.dir}/
			</outputDirectory>
            <includes>
                <include>${talend.job.path}/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- add test source codes -->
            <directory>${test.sourcecodes.dir}</directory>
            <outputDirectory>${talend.job.name}/src/test/java/</outputDirectory>
            <includes>
                <include>${talend.job.path}/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- add test context resources -->
            <directory>${test.resources.dir}</directory>
            <outputDirectory>${talend.job.name}/src/test/resources/
			</outputDirectory>
            <includes>
                <include>${talend.job.path}/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- junits reports -->
            <directory>${test.reports.dir}</directory>
            <outputDirectory>sunfire-reports</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- add all resources for running -->
            <directory>${contexts.running.dir}</directory>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
        </fileSet>
        <!-- If have children jobs, will add here -->
        <fileSet>
            <!-- microservice config files -->
            <directory>${microservice.configs.dir}</directory>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <includes>
                <include>config/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <!-- microservice running config files -->
            <directory>${microservice.running.configs.dir}</directory>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <includes>
                <include>config/**</include>
            </includes>
        </fileSet>
    </fileSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>lib</outputDirectory>
            <includes>
                <include>org.talend.libraries:avro-1.7.6-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-yarn-server-web-proxy-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hive-service-1.1.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:servlet-api-2.5:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-auth-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:zookeeper-3.4.5-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-common-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-mapreduce-client-jobclient-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-hdfs-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-mapreduce-client-common-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-yarn-client-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:derby-10.11.1.1:jar:6.0.0</include>
                <include>org.talend.libraries:hive-metastore-1.1.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-yarn-common-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.example.bigdata_project.code:routines:jar:7.3.1</include>
                <include>org.talend.libraries:jackson-module-paranamer-2.6.7:jar:6.0.0</include>
                <include>org.talend.libraries:hadoop-yarn-api-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hive-exec-1.1.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:hive-jdbc-1.1.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:guava-12.0.1:jar:6.0.0</include>
                <include>org.talend.libraries:commons-codec-1.9:jar:6.0.0</include>
                <include>org.talend.libraries:crypto-utils:jar:6.0.0-SNAPSHOT</include>
                <include>org.talend.libraries:hadoop-mapreduce-client-core-2.6.0-cdh5.12.1:jar:6.0.0</include>
                <include>org.talend.libraries:joda-time-2.9.3:jar:6.0.0</include>
                <include>org.talend.libraries:libthrift-0.9.2:jar:6.0.0</include>
            </includes>
            <outputFileNameMapping>${artifact.artifactId}.${artifact.extension}</outputFileNameMapping>
            <useProjectArtifact>false</useProjectArtifact>
        </dependencySet>
        <dependencySet>
            <outputDirectory>lib</outputDirectory>
            <includes>
                <include>org.apache.curator:curator-client:jar:2.6.0</include>
                <include>org.datanucleus:datanucleus-api-jdo:jar:3.2.6</include>
                <include>org.codehaus.jackson:jackson-jaxrs:jar:1.9.13</include>
                <include>org.antlr:antlr-runtime:jar:3.4</include>
                <include>org.apache.curator:curator-framework:jar:2.6.0</include>
                <include>commons-httpclient:commons-httpclient:jar:3.0.1</include>
                <include>commons-logging:commons-logging:jar:1.2</include>
                <include>org.codehaus.jackson:jackson-mapper-asl:jar:1.9.14-TALEND</include>
                <include>com.google.protobuf:protobuf-java:jar:2.5.0</include>
                <include>com.sun.jersey:jersey-core:jar:1.9</include>
                <include>org.apache.thrift:libfb303:jar:0.9.3</include>
                <include>com.fasterxml.jackson.core:jackson-annotations:jar:2.6.7</include>
                <include>org.slf4j:slf4j-api:jar:1.7.25</include>
                <include>org.apache.htrace:htrace-core:jar:3.2.0-incubating</include>
                <include>commons-collections:commons-collections:jar:3.2.2</include>
                <include>javax.jdo:jdo-api:jar:3.0.1</include>
                <include>org.apache.httpcomponents:httpclient:jar:4.5.2</include>
                <include>org.codehaus.jackson:jackson-core-asl:jar:1.9.14-TALEND</include>
                <include>com.sun.jersey:jersey-client:jar:1.9</include>
                <include>commons-cli:commons-cli:jar:1.2</include>
                <include>commons-configuration:commons-configuration:jar:1.6</include>
                <include>org.apache.logging.log4j:log4j-1.2-api:jar:2.12.1</include>
                <include>org.apache.logging.log4j:log4j-api:jar:2.12.1</include>
                <include>commons-net:commons-net:jar:2.2</include>
                <include>com.fasterxml.jackson.core:jackson-databind:jar:2.6.7.1</include>
                <include>org.apache.logging.log4j:log4j-core:jar:2.12.1</include>
                <include>org.apache.logging.log4j:log4j-slf4j-impl:jar:2.12.1</include>
                <include>org.datanucleus:datanucleus-rdbms:jar:3.2.9</include>
                <include>commons-io:commons-io:jar:2.4</include>
                <include>org.codehaus.jackson:jackson-xc:jar:1.9.13</include>
                <include>org.dom4j:dom4j:jar:2.1.1</include>
                <include>org.apache.htrace:htrace-core4:jar:4.0.1-incubating</include>
                <include>commons-lang:commons-lang:jar:2.6</include>
                <include>org.datanucleus:datanucleus-core:jar:3.2.10</include>
                <include>com.fasterxml.jackson.core:jackson-core:jar:2.6.7</include>
                <include>org.apache.httpcomponents:httpcore:jar:4.4.4</include>
            </includes>
            <useProjectArtifact>false</useProjectArtifact>
        </dependencySet>
        <dependencySet>
            <outputDirectory>${talend.job.name}</outputDirectory>
            <includes>
                <include>org.example.bigdata_project.job:Fact_Consu:jar:0.1.0</include>
            </includes>
            <outputFileNameMapping>${artifact.build.finalName}.${artifact.extension}</outputFileNameMapping>
            <useProjectArtifact>true</useProjectArtifact>
        </dependencySet>
    </dependencySets>
</assembly>
